-- A component type extension does not contain any non-unique identifiers.
-- The extended component type does not contain any identifiers. No bug SC0009.
PACKAGE COMPONENT_TYPE_EXTENSION_TC11
    PUBLIC
        THREAD thread_type_declaration        
        END thread_type_declaration;        
        
        THREAD thread_type_extension
            EXTENDS thread_type_declaration            
            PROTOTYPES
                id_1 : DATA;                
            FEATURES
                id_2 : OUT DATA PORT;                                                                    
        END thread_type_extension;        
END COMPONENT_TYPE_EXTENSION_TC11; 
