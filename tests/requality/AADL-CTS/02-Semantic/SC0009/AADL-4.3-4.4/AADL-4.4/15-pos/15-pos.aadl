-- identifiers. The implemented component type does not contain any identifiers.
-- No bug SC0009.
PACKAGE COMPONENT_IMPLEMENTATION_DECLARATION_TC15
PRIVATE
    THREAD threadT        
    END threadT;
    
    THREAD IMPLEMENTATION threadT.impl        
        SUBCOMPONENTS
            id_1 : SUBPROGRAM;        
        MODES
            id_2 : INITIAL MODE;            
    END threadT.impl;
    
END COMPONENT_IMPLEMENTATION_DECLARATION_TC15; 
