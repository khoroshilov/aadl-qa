
	-- Two aliases rename all declarations of the same package. The package does not contain the public section.
	-- No bug SC0024.
	PACKAGE TC6
	    PUBLIC
	        WITH pckgId::pckgId::pckgId;
	        renames pckgId::pckgId::pckgId::all; -- no bug SC0024    
	    PRIVATE
	        WITH pckgId::pckgId::pckgId;
	        renames pckgId::pckgId::pckgId::all; -- no bug SC0024    
	END TC6;

	 

	PACKAGE pckgId::pckgId::pckgId
	    PRIVATE
	        THREAD threadTypeId
	        END threadTypeId;
	END pckgId::pckgId::pckgId;
	
	 

