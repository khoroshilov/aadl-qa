-- A THREAD component type is declared with all subclauses specified.
-- The subclauses contain the none statement. No bug SC0006.
PACKAGE THREAD_TYPE_TC2
    PUBLIC
        THREAD T
            FEATURES
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END T;
        
        THREAD TExt
            EXTENDS T    
            FEATURES
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END TExt;
END THREAD_TYPE_TC2; 
