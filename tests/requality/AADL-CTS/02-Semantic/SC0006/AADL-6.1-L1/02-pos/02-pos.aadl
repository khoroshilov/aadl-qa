-- A PROCESSOR component type is declared with all subclauses specified.
-- The subclauses contain the none statement. No bug SC0006.
PACKAGE PROCESSOR_TYPE_TC2
    PUBLIC
        PROCESSOR T
            FEATURES
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END T;
        
        PROCESSOR TExt
            EXTENDS T    
            FEATURES
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END TExt;
END PROCESSOR_TYPE_TC2; 
