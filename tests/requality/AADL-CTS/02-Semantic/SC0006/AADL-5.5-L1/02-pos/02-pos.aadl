-- A THREAD GROUP component type is declared with all subclauses specified.
-- The subclauses contain the none statement. No bug SC0006.
PACKAGE THREAD_GROUP_TYPE_TC2
    PUBLIC
        THREAD GROUP T
            FEATURES
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END T;
        
        THREAD GROUP TExt
            EXTENDS T    
            FEATURES
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END TExt;
END THREAD_GROUP_TYPE_TC2; 
