-- A PROCESS component type is declared with all subclauses specified.
-- The subclauses contain the none statement. No bug SC0006.
PACKAGE PROCESS_TYPE_TC2
    PUBLIC
        PROCESS T
            FEATURES
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END T;
        
        PROCESS TExt
            EXTENDS T    
            FEATURES
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END TExt;
END PROCESS_TYPE_TC2; 
