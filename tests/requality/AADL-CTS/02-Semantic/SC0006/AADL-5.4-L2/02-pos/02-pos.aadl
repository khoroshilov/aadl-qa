-- A THREAD implementation is declared with all subclauses specified.
-- The subclauses contain the none statement. No bug SC0006.
PACKAGE THREAD_IMPLEMENTATION_TC2
    PUBLIC
        THREAD T                        
        END T;
        
        THREAD IMPLEMENTATION T.I
            SUBCOMPONENTS
                none;
            CALLS
                none;
            CONNECTIONS
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END T.I;
        
        THREAD IMPLEMENTATION T.IExt
            EXTENDS T.I
            SUBCOMPONENTS
                none;
            CALLS
                none;
            CONNECTIONS
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END T.IExt;
END THREAD_IMPLEMENTATION_TC2; 
