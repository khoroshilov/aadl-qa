-- A DEVICE implementation is declared with all subclauses specified.
-- The subclauses contain the none statement. No bug SC0006.
PACKAGE DEVICE_IMPLEMENTATION_TC2
    PUBLIC
        DEVICE T                        
        END T;
        
        DEVICE IMPLEMENTATION T.I
            SUBCOMPONENTS
                none;
            CALLS
                none;
            CONNECTIONS
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END T.I;
        
        DEVICE IMPLEMENTATION T.IExt
            EXTENDS T.I
            SUBCOMPONENTS
                none;
            CALLS
                none;
            CONNECTIONS
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END T.IExt;
END DEVICE_IMPLEMENTATION_TC2; 
