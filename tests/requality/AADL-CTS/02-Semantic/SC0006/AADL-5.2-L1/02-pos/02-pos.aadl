
	-- A SUBPROGRAM component type is declared with all subclauses specified.
	-- The subclauses contain the none statement. No bug SC0006.
	PACKAGE SUBPROGRAM_TYPE_TC2
	    PUBLIC
	        SUBPROGRAM T
	            FEATURES
	                none;
	            FLOWS
	                none;
	            MODES
	                none;
	            PROPERTIES
	                none;
	        END T;
	        
	        SUBPROGRAM TExt
	            EXTENDS T    
	            FEATURES
	                none;
	            FLOWS
	                none;
	            MODES
	                none;
	            PROPERTIES
	                none;
	        END TExt;
	END SUBPROGRAM_TYPE_TC2;

