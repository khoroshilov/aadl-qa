-- An ABSTRACT component type is declared with all subclauses specified.
-- The subclauses contain the none statement. No bug SC0006.
PACKAGE ABSTRACT_TYPE_TC2
    PUBLIC
        ABSTRACT T
            FEATURES
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END T;
        
        ABSTRACT TExt
            EXTENDS T    
            FEATURES
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END TExt;
END ABSTRACT_TYPE_TC2;
