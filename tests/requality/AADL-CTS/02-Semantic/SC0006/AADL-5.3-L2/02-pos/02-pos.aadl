-- A SUBPROGRAM GROUP implementation is declared with all subclauses specified.
-- The subclauses contain the none statement. No bug SC0006.
PACKAGE SUBPROGRAM_GROUP_IMPLEMENTATION_TC2
    PUBLIC
        SUBPROGRAM GROUP T                        
        END T;
        
        SUBPROGRAM GROUP IMPLEMENTATION T.I
            SUBCOMPONENTS
                none;
            CALLS
                none;
            CONNECTIONS
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END T.I;
        
        SUBPROGRAM GROUP IMPLEMENTATION T.IExt
            EXTENDS T.I
            SUBCOMPONENTS
                none;
            CALLS
                none;
            CONNECTIONS
                none;
            FLOWS
                none;
            MODES
                none;
            PROPERTIES
                none;
        END T.IExt;
END SUBPROGRAM_GROUP_IMPLEMENTATION_TC2; 
